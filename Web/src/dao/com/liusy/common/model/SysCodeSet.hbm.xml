<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.liusy.common.model">
   <class name="SysCodeSet" table="SYS_CODESET" >
      <meta attribute="sync-DAO">true</meta>
      <id name="id" column="ID" type="integer" >
         <generator class="native"/>
      </id>
      <property name="csType" column="CS_TYPE" type="string" not-null="false" length="32" />
      <property name="csStatus" column="CS_STATUS" type="string" not-null="false" length="2" />
      <property name="enName" column="EN_NAME" type="string" not-null="false" length="32" />
      <property name="cnName" column="CN_NAME" type="string" not-null="false" length="32" />
      <property name="remark" column="REMARK" type="string" not-null="false" length="512" />
   </class>	
</hibernate-mapping>
